{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API for contact application",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seeds:generate": "sequelize-cli seed:generate --name",
    "seeds:run": "sequelize-cli db:seed:all",
    "seeds:delete": "sequelize-cli db:seed:undo:all",
    "seeds:revert-last": "sequelize-cli db:seed:undo --seed",
    "seeds:revert": "sequelize-cli db:seed:undo --seed"
  },
  "engines": {
    "node": "16.13.x"
  },
  "keywords": [],
  "author": "Mauricio Robinson",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "joi": "^17.6.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.24.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "cypress": "^10.10.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.5.1"
  }
}
